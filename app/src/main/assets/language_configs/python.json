{
  "name": "Python",
  "fileExtensions": ["py", "pyw"],
  "keywords": {
    "primary": [
      "and", "as", "assert", "break", "class", "continue", "def", "del",
      "elif", "else", "except", "exec", "finally", "for", "from", "global",
      "if", "import", "in", "is", "lambda", "not", "or", "pass", "print",
      "raise", "return", "try", "while", "with", "yield"
    ],
    "secondary": [
      "async", "await", "nonlocal"
    ],
    "types": [
      "int", "float", "str", "bool", "list", "tuple", "dict", "set",
      "frozenset", "bytes", "bytearray", "memoryview", "range",
      "object", "type", "super", "property", "staticmethod", "classmethod"
    ],
    "literals": ["True", "False", "None"],
    "functions": [
      "abs", "all", "any", "ascii", "bin", "bool", "bytearray", "bytes",
      "callable", "chr", "classmethod", "compile", "complex", "delattr",
      "dict", "dir", "divmod", "enumerate", "eval", "exec", "filter",
      "float", "format", "frozenset", "getattr", "globals", "hasattr",
      "hash", "help", "hex", "id", "input", "int", "isinstance",
      "issubclass", "iter", "len", "list", "locals", "map", "max",
      "memoryview", "min", "next", "object", "oct", "open", "ord",
      "pow", "print", "property", "range", "repr", "reversed", "round",
      "set", "setattr", "slice", "sorted", "staticmethod", "str", "sum",
      "super", "tuple", "type", "vars", "zip"
    ]
  },
  "patterns": {
    "singleLineComment": "#",
    "multiLineCommentStart": "\"\"\"",
    "multiLineCommentEnd": "\"\"\"",
    "stringDelimiters": ["\"", "'", "\"\"\"", "'''"],
    "numberPattern": "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b",
    "identifierPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
    "operatorPattern": "[+\\-*/=%<>!&|^~@]",
    "bracketPattern": "[\\[\\]{}()]",
    "functionCallPattern": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\(",
    "decoratorPattern": "@[a-zA-Z_][a-zA-Z0-9_]*"
  },
  "colors": {
    "keyword": "#569CD6",
    "secondaryKeyword": "#C586C0",
    "type": "#4EC9B0",
    "string": "#CE9178",
    "number": "#B5CEA8",
    "comment": "#6A9955",
    "operator": "#D4D4D4",
    "function": "#DCDCAA",
    "class": "#4EC9B0",
    "literal": "#569CD6",
    "identifier": "#D4D4D4",
    "annotation": "#C586C0"
  },
  "features": {
    "lineNumbers": true,
    "wordWrap": false,
    "highlightCurrentLine": true,
    "bracketMatching": true,
    "autoIndent": true,
    "indentSize": 4,
    "usesTabs": false,
    "caseSensitive": true
  }
}
