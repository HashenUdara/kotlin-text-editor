{
  "name": "C#",
  "fileExtensions": ["cs"],
  "keywords": {
    "primary": [
      "abstract", "as", "base", "bool", "break", "byte", "case", "catch", "char",
      "checked", "class", "const", "continue", "decimal", "default", "delegate",
      "do", "double", "else", "enum", "event", "explicit", "extern", "false",
      "finally", "fixed", "float", "for", "foreach", "goto", "if", "implicit",
      "in", "int", "interface", "internal", "is", "lock", "long", "namespace",
      "new", "null", "object", "operator", "out", "override", "params", "private",
      "protected", "public", "readonly", "ref", "return", "sbyte", "sealed",
      "short", "sizeof", "stackalloc", "static", "string", "struct", "switch",
      "this", "throw", "true", "try", "typeof", "uint", "ulong", "unchecked",
      "unsafe", "ushort", "using", "virtual", "void", "volatile", "while"
    ],
    "secondary": [
      "add", "alias", "ascending", "async", "await", "by", "descending", "dynamic",
      "equals", "from", "get", "global", "group", "into", "join", "let", "nameof",
      "on", "orderby", "partial", "remove", "select", "set", "unmanaged", "value",
      "var", "when", "where", "with", "yield"
    ],
    "types": [
      "bool", "byte", "sbyte", "char", "decimal", "double", "float", "int",
      "uint", "long", "ulong", "short", "ushort", "object", "string", "var",
      "dynamic", "List", "Dictionary", "Array", "IEnumerable", "IList", "ICollection",
      "IDictionary", "ISet", "Queue", "Stack", "HashSet", "SortedSet", "SortedDictionary",
      "ConcurrentDictionary", "Task", "ValueTask", "Nullable", "Tuple", "ValueTuple"
    ],
    "literals": ["true", "false", "null"],
    "functions": [
      "Console.WriteLine", "Console.Write", "Console.ReadLine", "Console.ReadKey",
      "Convert.ToString", "Convert.ToInt32", "Convert.ToDouble", "Convert.ToBoolean",
      "string.Format", "string.Join", "string.Split", "string.Contains", "string.StartsWith",
      "string.EndsWith", "Math.Max", "Math.Min", "Math.Abs", "Math.Round", "Math.Floor",
      "Math.Ceiling", "Math.Sqrt", "Math.Pow", "DateTime.Now", "DateTime.Today",
      "Guid.NewGuid", "Environment.NewLine", "Path.Combine", "File.ReadAllText",
      "File.WriteAllText", "Directory.Exists", "File.Exists"
    ],
    "constants": [
      "Environment.NewLine", "string.Empty", "DateTime.MinValue", "DateTime.MaxValue",
      "int.MinValue", "int.MaxValue", "double.MinValue", "double.MaxValue",
      "float.MinValue", "float.MaxValue", "decimal.MinValue", "decimal.MaxValue"
    ]
  },
  "patterns": {
    "singleLineComment": "//",
    "multiLineCommentStart": "/*",
    "multiLineCommentEnd": "*/",
    "stringDelimiters": ["\"", "'", "@\"", "$\""],
    "numberPattern": "\\b\\d+(\\.\\d+)?([fFdDmM]|[eE][+-]?\\d+)?\\b",
    "identifierPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
    "operatorPattern": "[+\\-*/=%<>!&|^~?:]",
    "bracketPattern": "[\\[\\]{}()]",
    "functionCallPattern": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\(",
    "classPattern": "\\bclass\\s+([A-Z][a-zA-Z0-9_]*)",
    "importPattern": "\\busing\\s+([a-zA-Z0-9_.]+)",
    "annotationPattern": "\\[[a-zA-Z][a-zA-Z0-9_]*\\]"
  },
  "colors": {
    "keyword": "#569CD6",
    "secondaryKeyword": "#C586C0",
    "type": "#4EC9B0",
    "string": "#CE9178",
    "number": "#B5CEA8",
    "comment": "#6A9955",
    "operator": "#D4D4D4",
    "function": "#DCDCAA",
    "class_": "#4EC9B0",
    "literal": "#569CD6",
    "identifier": "#9CDCFE",
    "annotation": "#FFCB6B"
  },
  "features": {
    "caseSensitive": true,
    "indentSize": 4,
    "usesTabs": false,
    "autoIndent": true,
    "bracketMatching": true,
    "lineNumbers": true,
    "wordWrap": false,
    "highlightCurrentLine": true
  }
}
