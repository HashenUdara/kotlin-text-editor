{
  "name": "Java",
  "fileExtensions": ["java"],
  "keywords": {
    "primary": [
      "class", "interface", "enum", "abstract", "extends", "implements",
      "public", "private", "protected", "static", "final", "synchronized",
      "volatile", "transient", "native", "strictfp",
      "if", "else", "switch", "case", "default", "for", "while", "do",
      "break", "continue", "return", "try", "catch", "finally", "throw", "throws",
      "new", "this", "super", "instanceof", "package", "import"
    ],
    "secondary": [
      "assert", "const", "goto"
    ],
    "types": [
      "void", "boolean", "byte", "short", "int", "long", "float", "double", "char",
      "String", "Object", "Class", "Integer", "Boolean", "Character", "Double",
      "Float", "Long", "Short", "Byte", "List", "Set", "Map", "ArrayList",
      "HashMap", "HashSet", "LinkedList", "Vector", "Stack", "Queue"
    ],
    "literals": ["true", "false", "null"],
    "functions": [
      "System.out.println", "System.out.print", "System.err.println",
      "equals", "hashCode", "toString", "clone", "finalize",
      "wait", "notify", "notifyAll", "getClass"
    ]
  },
  "patterns": {
    "singleLineComment": "//",
    "multiLineCommentStart": "/*",
    "multiLineCommentEnd": "*/",
    "stringDelimiters": ["\"", "'"],
    "numberPattern": "\\b\\d+(\\.\\d+)?[fFdDlL]?\\b",
    "identifierPattern": "\\b[a-zA-Z_$][a-zA-Z0-9_$]*\\b",
    "operatorPattern": "[+\\-*/=%<>!&|^~?:]",
    "bracketPattern": "[\\[\\]{}()]",
    "functionCallPattern": "\\b([a-zA-Z_$][a-zA-Z0-9_$]*)\\s*\\(",
    "annotationPattern": "@[a-zA-Z_$][a-zA-Z0-9_$]*"
  },
  "colors": {
    "keyword": "#569CD6",
    "secondaryKeyword": "#C586C0",
    "type": "#4EC9B0",
    "string": "#CE9178",
    "number": "#B5CEA8",
    "comment": "#6A9955",
    "operator": "#D4D4D4",
    "function": "#DCDCAA",
    "class": "#4EC9B0",
    "literal": "#569CD6",
    "identifier": "#D4D4D4",
    "annotation": "#C586C0"
  },
  "features": {
    "lineNumbers": true,
    "wordWrap": false,
    "highlightCurrentLine": true,
    "bracketMatching": true,
    "autoIndent": true,
    "indentSize": 4,
    "usesTabs": false,
    "caseSensitive": true
  }
}
